<div class="col s12 m5">
   <div class="card blue-grey darken-1" style="overflow; height:227px;">
      <div class="card-content white-text">
         <div id = "data">
            <table id = "connected_users">
        <thead>
          <tr>
              <th>Name</th>
              <th>Score</th>
          </tr>
          <tbody>
            <% @connected.each do |conn| %>
          <tr>
            <td><%=conn.username%></td>
            <td><%=conn.score%></td>
          </tr>
          <% end %>
       </tbody>
    </thead>
  </table>
   </div>
      </div>
   </div>
</div>