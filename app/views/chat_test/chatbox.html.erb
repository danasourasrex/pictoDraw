<h1>Messages</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td><%= message.username %></td>
        <td><%= message.message %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>